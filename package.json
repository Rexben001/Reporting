{
  "name": "reporting",
  "version": "1.0.0",
  "description": "Whistle blowing app",
  "main": "app.js",
  "scripts": {
    "build": "babel server -d dist",
    "start": "npm run build && nodemon dist/app.js",
    "start-dev": "nodemon --exec babel-node server/app.js",
    "test": "nyc mocha --require @babel/register ./server/specs/*.js --timeout 10000 --exit",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint server/ --fix",
    "docs": "./node_modules/.bin/jsdoc server/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rexben001/Reporting.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Rexben001/Reporting/issues"
  },
  "homepage": "https://github.com/Rexben001/Reporting#readme",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.7",
    "nyc": "^13.1.0"
  }
