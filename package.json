  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "bcryptjs": "2.4.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-validator": "^5.3.0",
